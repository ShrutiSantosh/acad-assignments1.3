# acad-assignments1.3
Swapping of  two variables without temporary or 3rd variable

public class SwapingTwoNumbersWithoutTemporaryVarible {

	public static void main(String[] args) {
		int x = 30;
		int y = 40;

		// --1st method--//
		x = x + y; // x = 30+40 = 70
		// System.out.println(x);

		y = x - y; // y = 70-40 = 30
		System.out.println("val of y " + y);

		x = x - y; // x = 70-30 = 40
		System.out.println("val of x " + x);

		// --2nd method--//
		int a = 30;
		int b = 40;

		a = a * b; // a = 30*40 = 1200
		// System.out.println(a);

		b = a / b; // b = 1200/40 = 30
		System.out.println("val of b " + b);

		a = a / b; // a = 1200/30 = 40
		System.out.println("val of a " + a);
	}

}


output
val of y 30
val of x 40
val of b 30
val of a 40
